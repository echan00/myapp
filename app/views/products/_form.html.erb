<%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :url %>
    <%= f.input :name %>
    <%= f.input :image %>
    <%= f.input :title %>
    <%= f.input :createdby %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
